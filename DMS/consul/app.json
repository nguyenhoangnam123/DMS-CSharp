{
  "service": {
    "name": "dms-backend",
    "port": 80,
    "tags": [
      "dms-backend"
    ],
    "connect": {
      "sidecar_service": {
        "proxy": {
          "upstreams": [
            {
              "destination_name": "utils-backend",
              "local_bind_port": 10100
            }
          ]
        }
      }
    },
    "check": {
      "id": "dms-backend",
      "name": "dms-backend TCP on port 80",
      "tcp": "localhost:80",
      "interval": "5s",
      "timeout": "1s"
    }
  }
}
