{
  "service": {
    "name": "store-app-backend",
    "port": 80,
    "tags": [
      "store-app-backend"
    ],
    "connect": {
      "sidecar_service": {
        "proxy": {
          "upstreams": [
            {
              "destination_name": "es",
              "local_bind_port": 10008
            },
            {
              "destination_name": "windows-mssql",
              "local_bind_port": 1433
            },
            {
              "destination_name": "utils-backend",
              "local_bind_port": 10100
            },
            {
              "destination_name": "rabbitmq",
              "local_bind_port": 5672
            }
          ]
        }
      }
    },
    "check": {
      "id": "store-app-backend",
      "name": "store-app-backend TCP on port 80",
      "tcp": "localhost:80",
      "interval": "5s",
      "timeout": "1s"
    }
  }
}
